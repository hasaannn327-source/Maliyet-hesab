{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [{ "key": "cache-control", "value": "s-maxage=31536000, immutable" }]
    }
  ],
  "env": {
    "TSC_COMPILE_ON_ERROR": "true",
    "GENERATE_SOURCEMAP": "false"
  }
}
